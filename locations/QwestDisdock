# QwestDisdock
!!in location stat at any place
if qwestonce = 0:
	qwestonce = 1
	!!ADDQST 'qwest/qwest.qsp'
end

!!reference to a transition
!!located in the location park  array luna
if vladimirQW = 0 and hour >= 9 and hour <= 20 and week > 5:'<a href="exec:gt ''qwloc''">Near the fence strong man stands and looks at you</a>.'
!!in location korr
if vladimirday ! daystart and vladimirQW = 20 and hour = 16:'<a href="exec:gt ''qwloc2''">The phone rings</a>.'
!!in location telefon array start
if vladimirday ! daystart and vladimirQW = 20 and hour = 16:gt 'qwloc2'
!!in location restoran array start
if vladimirday = daystart and vladimirQW = 21 and hour = 20:gt 'qwmeet'
!!in location korr
if vladimirday ! daystart and vladimirQW = 25 and hour = 16 and week = 6:'<a href="exec:gt ''qwloc3''">The phone rings</a>.'
!!in location telefon array start
if vladimirday ! daystart and vladimirQW = 25 and hour = 16 and week = 6:gt 'qwloc3'
!!street
if vladimirday = daystart and vladimirQW = 30 and hour >= 16 and week = 6:'<a href="exec:gt ''qwmeet2''">Before your porch worth audit and near stolidly worth waiting for you driver Glory</a>.'

!!down
if izoldaQW >= 1:'<a href="exec:gt ''qwIzoldaApp''">Flat Isolde</a>.'
!!nord
if slavaQW >= 1:'<a href="exec:gt ''qwBarPolet''">Bar "Flight"</a>.'

